<div class="inputs">
<h3>New Dealer Store</h3>
<%= simple_form_for(@dealer_store) do |f| %>
  <%= f.input :branch_name %>
  <%= f.input :address %>
  <%= f.association :dealer %>
  <%= f.submit "Create Dealer Store", :class => "mainbutton wfont" %>
  <button class="hiddenbutton lmargin"><%= link_to "Back to dealer stores", dealer_stores_path, :class => "blfont" %></button>
<% end %>
</div>