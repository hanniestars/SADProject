<div class="container">
  <div id="normaltable" class="fourfifth wrap">
      <!--table searh bar-->
      <form class="search">
              <input type="text" placeholder="Search Keyword">
            <button class="lmargin wfont" type="submit">Search</button>
          </form>
      <table id="salescontent" class="sortable">
        <thead>
          <!--table headers-->
          <tr>
            <th class="wfont">Branch Name</th>
            <th class="wfont">Address</th>
            <th class="wfont">Dealer Name</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @dealer_stores.each do |dealer_store| %>
          <tr>
            <td><%= dealer_store.branch_name %></td>
            <td><%= dealer_store.address %></td>
            <td><%= dealer_store.dealer.dealer_name %></td>
            <td><%= link_to "Destroy", dealer_store_path(dealer_store.id), :method => :delete, :data => { :confirm => "Are you sure?", :class => "strong" } %>
            <% end %></td>
          </tr>
        </tbody>
      </table>
    </div>
</div>