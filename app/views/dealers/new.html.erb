<div class="inputs">
<h3>New Dealer</h3>
<%= simple_form_for(@dealer) do |f| %>
  <%= f.input :dealer_name %>
  <%= f.input :description %>
  <%= f.submit "Create Dealer", :class => "mainbutton wfont" %>
  <button class="hiddenbutton lmargin"><%= link_to "Back to dealers", dealers_path, :class => "blfont" %></button>
<% end %>
</div>